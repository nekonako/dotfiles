{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.previewAutoClose": true,
  "coc.preferences.rootPatterns": [".git", ".hg", ".projections.json", ".vim"],
  "coc.source.buffer.ignoreGitignore": false,
  "codeLens.enable": true,
  "codeLens.separator": "‚ù±",
  "diagnostic.enable": true,
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "EE",
  "diagnostic.hintSign": "HH",
  "diagnostic.infoSign": "II",
  "diagnostic.messageTarget": "float",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualText": false,
  "diagnostic.warningSign": "WW",
  "emmet.includeLanguages": { "vue-html": "html", "javascript": "javascriptreact", "jinja2.html": "html" },
  "emmet.showExpandedAbbreviation": "always",
  "go.goplsOptions": { "usePlaceholders": true, "completeUnimported": true },
  "highlight.colors.enable": false,
  "highlight.document.enable": true,
  "html.filetypes": ["html", "handlebars", "jinja2.html", "php"],
  "html.suggest.html5": true,
  "html.validate.styles": true,
  "languageserver": { 
	"ccls": { 
		"command": "ccls", 
		"filetypes": ["c", "cpp", "objc", "objcpp"], 
		"rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"], 
		"initializationOptions": { 
			"cache": { 
				"directory": "/tmp/ccls" 
			},
			"client": {
				"snippetSupport": true
			}
		} 
	},
	"golang": {
      		"command": "gopls",
      		"rootPatterns": ["go.mod"],
      		"disableWorkspaceFolders": true,
      		"filetypes": ["go"]
    }
 },
  "latex.build.args": [ "-xelatex", "-file-line-error", "-synctex=1", "-interaction=nonstopmode" ],
  "latex.build.executable": "latexmk",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.printWidth": 500,
  "python.analysis.diagnosticEnabled": false,
  "python.autoComplete.addBrackets": false,
  "python.autoUpdateLanguageServer": false,
  "python.formatting.blackArgs": ["--line-length=80"],
  "python.formatting.blackPath": "/usr/bin/black",
  "python.formatting.provider": "black",
  "python.formatting.yapfPath": "/usr/bin/yapf",
  "python.jediEnabled": false,
  "python.linting.enabled": true,
  "python.linting.pydocstyleArgs": ["--ignore=D100"],
  "python.linting.pydocstyleEnabled": false,
  "python.linting.pydocstylePath": "/usr/bin/pydocstyle",
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "/usr/bin/pylint",
  "python.linting.pylintUseMinimalCheckers": true,
  "python.pipenvPath": "/usr/bin/pipenv",
  "python.pythonPath": "python",
  "python.terminal.activateEnvironment": true,
  "signature.enable": true,
  "signature.hideOnTextChange": true,
  "signature.target": "float",
  "signature.triggerSignatureWait": 50,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.autoTrigger": "always",
  "suggest.floatEnable": true,
  "suggest.highPrioritySourceLimit": 90,
  "suggest.lowPrioritySourceLimit": 89,
  "suggest.minTriggerInputLength": 1,
  "suggest.noselect": true,
  "suggest.timeout": 2000,
  "suggest.triggerAfterInsertEnter": true,
  "vimlsp.indexes.count": 2,
  "vimlsp.indexes.runtimepath": true,
  "vimlsp.suggest.fromRuntimepath": true
}
